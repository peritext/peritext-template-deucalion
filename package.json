{
  "name": "peritext-template-deucalion",
  "version": "0.1.2",
  "description": " a web template designed dedicated to long reading experiences and context explorations, for the peritext ecosystem",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist; mkdir dist; babel src -d dist",
    "addbuild": "git add dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "pre-commit": [
    "lint",
    "build",
    "addbuild"
  ],
  "author": "Robin de Mourat",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/peritext/peritext-template-deucalion/issues"
  },
  "homepage": "https://github.com/peritext/peritext-template-deucalion#readme",
  "peerDependencies": {
    "react": "^16.7.0-alpha",
    "react-dom": "^16.7.0-alpha",
    "lodash": "^4.17"
  },
  "dependencies": {
    "d3-array": "^2.0.3",
    "d3-drag": "^1.2.3",
    "d3-ease": "^1.0.5",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.3.2",
    "d3-zoom": "^1.7.3",
    "google-map-react": "^1.1.2",
    "object-path": "^0.11.4",
    "peritext-schemas": "^0.1.1",
    "peritext-utils": "^0.1.0",
    "react-citeproc": "^0.1.10",
    "react-custom-scrollbars": "^4.2.1",
    "react-d3-graph": "^2.0.0-rc2",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^4.3.1",
    "react-sizeme": "^2.5.2",
    "redraft": "^0.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^8.2.2",
    "@robindemourat/eslint-config": "git+https://github.com/robindemourat/eslint-config.git",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "pre-commit": "^1.2.2",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.5.1",
    "react": "^16.7.0-alpha",
    "react-dom": "^16.7.0-alpha"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ]
  }
}
